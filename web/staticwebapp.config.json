{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/images/*.{png,jpg,gif}",
            "/css/*",
            "/js/*",
            "/assets/*"
        ]
    },
    "mimeTypes": {
        ".json": "application/json"
    },
    "globalHeaders": {
        "content-security-policy": "default-src https: 'unsafe-inline' 'unsafe-eval'; img-src 'self' data: https:;"
    }
}